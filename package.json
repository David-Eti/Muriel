{
  "name": "Muriel",
  "version": "1.0.0",
  "description": "A cross-platform app to binge-watch anything, without being bored",
  "homepage": "https://github.com/OrkoHunter/Muriel",
  "author": "Himanshu Mishra (@OrkoHunter)",
  "repository": {
    "type": "git",
    "url": "git://github.com/documentcloud/underscore.git"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "build -mwl --x64",
    "package": "electron-packager ./ Muriel --platform=darwin,linux,win32 --arch=x64 --out ~/Desktop/Muriel --app-version 1.0.0 --electron-version 1.6.2 --overwrite --icon=./app/img/bing.ico"
  },
  "devDependencies": {
    "electron": "^1.6.2",
    "electron-packager": "^8.5.2",
    "electron-builder": "16.4.2"
  },
  "dependencies": {
    "crypto": "0.0.3",
    "open": "0.0.5",
    "walk": "^2.3.9",
    "electron-reload": "^1.1.0"
  },
  "build": {
    "appId": "net.orkohunter.muriel",
    "productName": "Muriel",
    "electronVersion": "1.6.2",
    "mac": {
      "category": "public.app-category.entertainment",
      "icon": "app/img/icons/bing.icns"
    }
  }
}
